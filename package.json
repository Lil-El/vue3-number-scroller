{
  "name": "@lil-el/vue3-number-scroller",
  "private": false,
  "version": "0.0.1-alpha.10",
  "type": "module",
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./css": "./dist/index.css",
    "./*": "./*"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "global.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm run build:es && pnpm run build:umd",
    "build:es": "vite build --mode es  && vue-tsc -b",
    "build:umd": "vite build --mode umd  && vue-tsc -b",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.4",
    "vue-tsc": "^2.2.12"
  },
  "author": "Mino",
  "homepage": "https://lil-el.github.io",
  "description": "A Vue 3 component for scrolling numbers with customizable options.",
  "keywords": [
    "vue3",
    "number scroller",
    "vue3 number scroller",
    "vue3 number scroller component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Lil-El/vue3-number-scroller.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
